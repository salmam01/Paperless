{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "/app/Logs/Paperless-.txt",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ]
  },
  "RabbitMq": {
    "Host": "paperless-rabbitmq",
    "Port": 5672,
    "User": "guest",
    "Password": "guest",
    "OcrQueue": "ocr.queue",
    "SummaryQueue": "summary.queue"
    "MaxRetries": 3
  },
  "MinIo": {
    "Endpoint": "paperless-minio:9000",
    "Username": "minioadmin",
    "Password": "minioadmin",
    "BucketName": "documents"
  },
  "Ocr": {
    "DefaultLanguage": "deu+eng",
    "DefaultOem": "LstmOnly",
    "DefaultPsm": "Auto",
    "DefaultDpi": 300,
    "UseDeskew": true,
    "UseAdaptiveThreshhold": true,
    "UseSharpen": true,
    "MaxPages": 50
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=paperless-db;Port=5432;Database=WHICH DATABASE?:);Username=DAS AUCH;Password=SALMA;)"
  },
  "GenAI": {
    "ApiKey": "ASK ZELIHA:)",
    "ModelName": "gemini-2.0-flash",
    "ApiUrl": "https://generativelanguage.googleapis.com/v1beta/models/{0}:generateContent",
    "MaxRetries": 3,
    "TimeoutSeconds": 30
  }
}
