{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "/app/Logs/Paperless-.txt",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ]
  },
  "RabbitMQ": {
    "Host": "paperless-rabbitmq",
    "Port": 5672,
    "User": "guest",
    "Password": "guest",
    "QueueName": "document.uploaded"
  },
  "Minio": {
    "Endpoint": "paperless-minio:9000",
    "Username": "minioadmin",
    "Password": "minioadmin",
    "BucketName": "documents"
  },
  "OCR": {
    "DefaultLanguage": "deu+eng",
    "DefaultOem": "LstmOnly",
    "DefaultPsm": "Auto",
    "DefaultDpi": 300,
    "UseDeskew": true,
    "UseAdaptiveThreshhold": true,
    "UseSharpen": true,
    "MaxPages": 50
  }
}
