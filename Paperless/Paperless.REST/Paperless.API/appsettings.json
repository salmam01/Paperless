{
  "ConnectionStrings": {
    "DefaultConnection": "Host=paperless-db;Port=5432;Database=PaperlessDB;Username=swen3;Password=paperless123"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/Paperless-.txt",
          "rollingInterval": "Day",
          "formatter": "Serilog.Formatting.Json.JsonFormatter"
        }
      }
    ]
  },
  "ConnectionString": "Host=paperless-db;Port=5432;Database=PaperlessDB;Username=swen3;Password=paperless123",
  "RabbitMQ": {
    "Host": "paperless-rabbitmq",
    "Port": 5672,
    "User": "admin",
    "Password": "admin123",
    "ExchangeName": "document.events"
  },
  "MQPublisher": {
    "RoutingKeys": [
      "document.uploaded",
      "document.deleted",
      "document.deleted.all"
    ]
  },
  "ElasticSearch": {
    "Url": "http://paperless-elasticsearch:9200",
    "Index": "documents",
    "Username": "",
    "Password": "admin123"
  },
  "MinIO": {
    "Endpoint": "paperless-minio:9000",
    "Username": "admin",
    "Password": "admin123",
    "BucketName": "documents"
  },
  "Categories": {
    "Names": [
      "School",
      "Work",
      "Medical",
      "Finance",
      "Legal",
      "Home",
      "Personal",
      "Technology",
      "Certificates",
      "Travel",
      "Other"
    ]
  },
    "AllowedHosts": "*"
  }
